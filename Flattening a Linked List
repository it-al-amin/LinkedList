Node *flatten(Node *root)
{
   // Your code here
   Node* head=NULL;
   if(root==NULL)
   {
       return head;
   }
   multiset<int>s;
   Node* temp=NULL;
   while(root!=NULL)
   {
       s.insert(root->data);//nexted data
       temp=root->bottom;
       while(temp!=NULL)
       {
           s.insert(temp->data);//bottom data
           temp=temp->bottom;
       }
       root=root->next;
       
   }
   for(auto i:s)
   {
       if(head==NULL)
       {
           head=new Node(i);
           root=head;
       }
       else
       {
           root->bottom=new Node(i);
           root=root->bottom;
       }
      
   }
   return head;
   
}
